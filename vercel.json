{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/create-checkout-session.js": {
      "maxDuration": 30
    },
    "api/webhook/stripe.ts": {
      "maxDuration": 30
    }
  }
}
